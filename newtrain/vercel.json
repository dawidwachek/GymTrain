{
    "builds": [{
        "src": "newtrain/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.11.3" }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "newtrain/wsgi.py"
    }]
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
